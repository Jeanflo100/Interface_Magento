<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="690.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ecofish.interface_magento.view.StatusProductOverviewController">
   <children>
      <Label layoutX="345.0" layoutY="517.0" text="CATEGORY :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="categoryComboBox" layoutX="311.0" layoutY="538.0" prefHeight="25.0" prefWidth="149.0" promptText="Select CATEGORY" />
