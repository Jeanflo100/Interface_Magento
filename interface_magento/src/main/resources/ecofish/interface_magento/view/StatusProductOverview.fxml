         </font>
      </Label>
      <ComboBox fx:id="familyComboBox" layoutX="311.0" layoutY="607.0" prefHeight="25.0" prefWidth="149.0" promptText="Select FAMILY" />
      <TableView fx:id="inactiveProductTable" layoutX="36.0" layoutY="34.0" prefHeight="466.0" prefWidth="308.0">
         <columns>
            <TableColumn prefWidth="541.0" text="Inactive products">
               <columns>
                  <TableColumn fx:id="nameInactiveProductColumn" prefWidth="190.0" sortable="false" text="Name" />
                  <TableColumn fx:id="qualityInactiveProductColumn" prefWidth="60.0" sortable="false" text="Quality" />
                  <TableColumn fx:id="sizeInactiveProductColumn" prefWidth="60.0" sortable="false" text="Size" />
               </columns>
            </TableColumn>
         </columns>
      </TableView>
      <Button fx:id="updatePriceButton" layoutX="532.0" layoutY="517.0" mnemonicParsing="false" onAction="#handleUpdatePriceButton" prefHeight="43.0" prefWidth="94.0" text="Update prices" />
