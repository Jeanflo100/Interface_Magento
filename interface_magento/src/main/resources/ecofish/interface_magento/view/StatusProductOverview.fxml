                  <TableColumn fx:id="nameActiveProductColumn" prefWidth="190.0" sortable="false" text="Name" />
                  <TableColumn fx:id="qualityActiveProductColumn" prefWidth="60.0" sortable="false" text="Quality" />
                  <TableColumn fx:id="sizeActiveProductColumn" prefWidth="60.0" sortable="false" text="Size" />
               </columns>
            </TableColumn>
         </columns>
      </TableView>
      <Button fx:id="leftToRightButton" layoutX="351.0" layoutY="242.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="69.0" text="&gt;&gt;&gt;&gt;&gt;&gt;" />
      <Button fx:id="rightToLeftButton" layoutX="351.0" layoutY="289.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="69.0" text="&lt;&lt;&lt;&lt;&lt;&lt;" />
